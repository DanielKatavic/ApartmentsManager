@model IList<DataLayer.Models.Apartment>
@{
    if (Model.Count == 0)
    {
        <div style="width: 100%; text-align: center; font-weight: 700">Sorry, there are no matching results</div>
    }
    foreach (var apartment in Model)
    {
        @Html.Partial("_ApartmentCardView", apartment);
    }
}
